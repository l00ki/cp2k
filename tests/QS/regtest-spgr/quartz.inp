 &GLOBAL
   PRINT_LEVEL  MEDIUM
   PROJECT_NAME quartz
   RUN_TYPE  CELL_OPT
 &END GLOBAL
 &MOTION
   &CELL_OPT
     OPTIMIZER  BFGS
     MAX_ITER  1
     KEEP_ANGLES  T
     KEEP_SYMMETRY  T
     KEEP_SPACE_GROUP T
     EPS_SYMMETRY 1e-4
   &END CELL_OPT
 &END MOTION
 &FORCE_EVAL
   METHOD  QS
   STRESS_TENSOR  ANALYTICAL
