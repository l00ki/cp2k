&GLOBAL
   PROJECT  O2-HF-uks
   PRINT_LEVEL LOW
   RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    AUTO_BASIS RI_XAS MEDIUM

    UKS
    MULTIPLICITY 3

    &POISSON
      PERIODIC NONE
      PSOLVER MT
    &END

    &QS
      METHOD GAPW
    &END QS

    &XC
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
      &HF
         FRACTION 1.0
      &END HF
    &END XC

    &XAS_TDP
      &DONOR_STATES
         DEFINE_EXCITED BY_KIND
         KIND_LIST O
         STATE_TYPES 1s
         N_SEARCH 2
         LOCALIZE
      &END DONOR_STATES

      EXCITATIONS OS_SPIN_CONS

      &GW2X
      &END GW2X

      &KERNEL
         &XC_FUNCTIONAL NONE
         &END XC_FUNCTIONAL
         &EXACT_EXCHANGE
            FRACTION 1.0
         &END EXACT_EXCHANGE
      &END KERNEL
    &END XAS_TDP
  &END DFT

  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
      PERIODIC NONE
    &END CELL
    &COORD
      O  0.00000000  0.00000000  -0.00423076
      O  0.00000000  -0.00000000  1.21423076
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
    &KIND O
      BASIS_SET 6-31G*
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
