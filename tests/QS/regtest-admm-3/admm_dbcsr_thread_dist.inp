  &DFT
    BASIS_SET_FILE_NAME BASIS_ZIJLSTRA
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    BASIS_SET_FILE_NAME BASIS_ADMM
    BASIS_SET_FILE_NAME BASIS_ADMM_MOLOPT
    BASIS_SET_FILE_NAME GTH_POTENTIALS
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-06
      EXTRAPOLATION PS
    &END QS
    &MGRID
      NGRIDS 5
      CUTOFF 100
      REL_CUTOFF 30
    &END MGRID
    &XC
      &XC_FUNCTIONAL PBE
        &PBE
          SCALE_X 0.0
          SCALE_C 1.0
        &END PBE
        &XWPBE
          OMEGA 0.11
          SCALE_X -0.25
          SCALE_X0 1.0
        &END XWPBE
      &END XC_FUNCTIONAL
      &HF
        FRACTION 0.25
        &SCREENING
          EPS_SCHWARZ 1.0E-2
          SCREEN_ON_INITIAL_P T
        &END SCREENING
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE SHORTRANGE
          OMEGA 0.11
        &END INTERACTION_POTENTIAL
        &MEMORY
          MAX_MEMORY 16000
          EPS_STORAGE_SCALING 0.1
        &END MEMORY
      &END HF
    &END XC
    &SCF
