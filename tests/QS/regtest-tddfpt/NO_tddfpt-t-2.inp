&GLOBAL
  PROJECT NO_tddfpt-t-2
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &PROPERTIES
    &TDDFPT
       NSTATES      3
       MAX_ITER    10
       MAX_KV      10
       CONVERGENCE 1.0e-5
       OE_CORR SAOP
       RKS_TRIPLETS T
       &XC
         &XC_FUNCTIONAL PADE
         &END XC_FUNCTIONAL
         &XC_GRID
           XC_DERIV NN4_SMOOTH
         &END XC_GRID
       &END XC
    &END TDDFPT
  &END PROPERTIES

  &DFT
    LSD T
    MULTIPLICITY 2
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 260
    &END MGRID
    &QS
    &END QS
    &SCF
      MAX_SCF 15
      SCF_GUESS atomic
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
    &END CELL
    &COORD
     O   0.100000    0.000000    0.542338
     N   0.000000    0.300000   -0.619815
    &END COORD
    &KIND N
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q5
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
