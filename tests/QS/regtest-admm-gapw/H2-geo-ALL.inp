&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 150
      REL_CUTOFF 30
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD MO_DIAG
    &END
    &POISSON
      PERIODIC NONE
      PSOLVER MT
    &END
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC
      MAX_SCF 10
      &OT ON
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL
        &PBE
          SCALE_X 0.75
          SCALE_C 1.0
        &END
      &END XC_FUNCTIONAL
      &HF
        &SCREENING
          EPS_SCHWARZ 1.0E-6
          EPS_SCHWARZ_FORCES 1.0E-6
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE COULOMB
        &END
        FRACTION 0.25
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 8.0 8.0 8.0
      PERIODIC NONE
    &END CELL
    &COORD
      H  0.00000 0.00000 0.00000
      H  0.00000 0.00000 1.00000
    &END COORD
    &KIND H
      BASIS_SET Ahlrichs-def2-TZVP
      BASIS_SET AUX_FIT Ahlrichs-def2-SVP
      POTENTIAL ALL
    &END KIND
    &KIND C
      BASIS_SET Ahlrichs-def2-TZVP
      BASIS_SET AUX_FIT Ahlrichs-def2-SVP
      POTENTIAL ALL
     &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2-geo-ALL
  PRINT_LEVEL MEDIUM
  RUN_TYPE GEO_OPT
&END GLOBAL
&MOTION
   &GEO_OPT
      MAX_ITER 2
   &END GEO_OPT
&END MOTION
