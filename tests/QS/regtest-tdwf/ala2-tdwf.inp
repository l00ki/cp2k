&FORCE_EVAL
  METHOD QUICKSTEP
  &DFT
    &LOCALIZE
      OUT_ITER_EACH 1
      &PRINT
        &WANNIER_CENTERS
          ADD_LAST NUMERIC
          COMMON_ITERATION_LEVELS 5
          FILENAME ""
          IONS+CENTERS .TRUE.
        &END
        &WANNIER_STATES
          ADD_LAST NUMERIC
          COMMON_ITERATION_LEVELS 5
          FILENAME ""
        &END
      &END
    &END
    &REAL_TIME_PROPAGATION
       MAX_ITER 5
       MAT_EXP TAYLOR
       EPS_ITER 1.0E-9
       INITIAL_WFN SCF_WFN
       APPLY_DELTA_PULSE .TRUE.
       DELTA_PULSE_DIRECTION 1 0 0
    &END
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 100
    &END MGRID
    &PRINT
      &MOMENTS
        ADD_LAST NUMERIC
        COMMON_ITERATION_LEVELS 5
        FILENAME ""
        COM_NL .TRUE.
        MAGNETIC .TRUE.
        MAX_MOMENT 2
        PERIODIC .FALSE.
        REFERENCE COM
        VEL_REPRS .TRUE.
      &END
    &END
    &QS
      EPS_DEFAULT 1.0E-10
    &END QS
    &SCF
      MAX_SCF 60
      EPS_SCF 1.0E-3
      SCF_GUESS ATOMIC
      &OUTER_SCF
      EPS_SCF 1.0E-3
      &END
      &OT ON
        ROTATION
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
